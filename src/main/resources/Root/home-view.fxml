<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="337.0" prefWidth="728.0" xmlns="http://javafx.com/javafx/17.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Root.HomeController">
   <children>
      <Label alignment="CENTER" prefHeight="57.0" prefWidth="232.0" text="Welcome to GPA Tracker!!" />
      <Button alignment="CENTER" mnemonicParsing="false" onAction="#onAddSubjectClick" prefHeight="25.0" prefWidth="265.0" text="Add Subject">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin></Button>
      <Button alignment="CENTER" mnemonicParsing="false" onAction="#removeSubject" prefHeight="25.0" prefWidth="265.0" text="Remove Subject">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </Button>
      <Button alignment="CENTER" mnemonicParsing="false" onAction="#loadGraph" prefHeight="25.0" prefWidth="265.0" text="Load Graph">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin></Button>
      <Button alignment="CENTER" mnemonicParsing="false" onAction="#getSubjectList" prefHeight="25.0" prefWidth="265.0" text="Subject List">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin></Button>
      <Button alignment="CENTER" mnemonicParsing="false" onAction="#onResetClick" prefHeight="25.0" prefWidth="265.0" text="Reset Data" />
   </children>
</VBox>
